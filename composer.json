{
    "name": "popov654/php-html-parser",
    "type": "library",
    "description": "An HTML DOM parser. It allows you to manipulate HTML. Find tags on an HTML page with selectors just like jQuery.",
    "keywords": ["html", "dom", "parser"],
    "homepage": "https://github.com/popov654/php-html-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Popov",
            "email": "popov654.spb@gmail.com",
            "homepage": "https://popov654.pp.ru"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "ext-curl": "*",
        "popov654/string-encoder": ">=1.0.2",
        "php-http/httplug": "^2.1",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^1.6",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "mockery/mockery": "^1.2",
        "infection/infection": ">=0.13.4",
        "phan/phan": ">=2.4",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": { 
        	"PHPHtmlParser\\": "src/PHPHtmlParser"
       	}
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
